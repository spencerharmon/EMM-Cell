/**
Indicates that the cell should not be moving.

 \color a1b
 \radius 2
 */

element EAntiMove : QEnzyme {
  @Override Void specialEnzymeBehavior(TransitionSelf & ts){
    for(SiteNum i = 1; ew.isAccessible(i); ++i){
      Atom a = ew[i];
      if(a as EMove){
        ew[i] = self.atomof;
      }else if(a as ESymmetry){
        ew[i] = self.atomof;
      }
    }
  }
}