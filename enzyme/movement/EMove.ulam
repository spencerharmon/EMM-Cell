/**
Indicates that the cell should be moving.

 \color b1a
 \radius 2
 */

element EMove : QEnzyme {
  @Override Void specialEnzymeBehavior(TransitionSelf & ts){
    for(SiteNum i = 1; ew.isAccessible(i); ++i){
      Atom a = ew[i];
      if(a as ESymmetry){
        ts.changeSelf(a.atomof);
      }else if(a as EAntiMove){
        ts.changeSelf(a.atomof);
      }
    }
  }
}