element EDeath : QEnzyme{
  typedef EventWindow.SiteNum SiteNum;
  Void specialEnzymeBehavior(){
    for(SiteNum i = 0; i < ew.size(); ++i){
      Atom a = ew[i];
      
      Empty e;
      if(a as QGene){
        ew[i] = e;
      }else if(a as QEnzyme){
        ew[i] = e;
      }
      if(i <= 4){
        if(a as InnerMembrane){
          ew[i] = self;
        }
      }
    }
  }
}
