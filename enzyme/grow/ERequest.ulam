/**
 \radius 2
 */
element ERequest : QGrow {
  TreeID treeID;
  GeneID geneID;
  Bool isRoot;
  
  @Override TreeID getTargetTreeID(){
    return treeID;
  }
  @Override GeneID getTargetGeneID(){
    return geneID;
  }
  @Override Bool getTargetIsRoot(){
    return isRoot;
  }

  @Override Bool individualDeathBehavior(){
    EGrow eg;
    eg.treeID = treeID;
    eg.geneID = geneID;
    eg.isRoot = isRoot;
    ew[0] = eg.atomof;
    return true;
  }
}