quark QTreeCopy : QGeneCopy {
  //careful; not valid for root copy
  virtual Bool & getSeenByParent();
  virtual GeneID getGeneID(){
    return (GeneID) 0;
  }
  virtual TreeID getTreeID(){
    return (TreeID) 0;
  }
  virtual Void specialTreeCopyBehavior();
  @Override Void specialGeneCopyBehavior(){
    specialTreeCopyBehavior();
  }
}