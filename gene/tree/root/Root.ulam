/**
 Empty root element.
 \radius 2
 */
element Root : QTree {
  //data members
  TreeID treeID;
  GeneID tree0;
  GeneID tree1;
  OperatorType op;
  StoredValue value;

  OperatorUtils ou;

  @Override TreeID getTreeID(){
    return treeID;
  }
  @Override StoredValue getStoredValue(){
    return value;
  }
  @Override Void specialBehavior(FinalEWState & fs){
    value = ou.doOperate(tree0, tree1, op, treeID, value);
  }
}